{
  "name": "ipfs-webui",
  "version": "0.3.0",
  "description": "A frontend for IPFS",
  "main": "index.jsx",
  "dependencies": {
    "bootstrap": "3.3.4",
    "browser-request": "^0.3.3",
    "debug": "^2.2.0",
    "font-awesome": "4.3.0",
    "ipfs-geoip": "^1.4.3",
    "jquery": "^2.1.3",
    "lodash": "3.6.0",
    "react": "0.13.1"
  },
  "devDependencies": {
    "browserify": "10.2.3",
    "ipfs-web-app": "^0.1.6",
    "kew": "^0.6.0",
    "less": "2.5.0",
    "pre-commit": "1.0.6",
    "reactify": "1.1.1",
    "recursive-readdir": "^1.2.1",
    "standard": "3.3.2",
    "stream-sink": "^1.2.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node buildapps.js ; ipfs-web-app run",
    "lint": "git diff --name-only --cached --relative | egrep .jsx?$ | xargs --no-run-if-empty standard"
  },
  "standard": {
    "ignore": [
      "**/dist/",
      "**/node_modules/",
      "**/include/",
      "versions",
      "js/include"
    ]
  },
  "pre-commit": [
    "lint"
  ],
  "author": "Juan Benet <juan@benet.ai> (http://juan.benet.ai/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipfs/webui"
  },
  "ipfs-web-app": {
    "build": {
      "dist/script.js": "browserify -t reactify index.jsx",
      "dist/style.css": "lessc style/style.less"
    }
  },
  "keywords": [
    "ipfs",
    "webui"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipfs/webui/issues"
  },
  "homepage": "https://github.com/ipfs/webui"
}
